{
  "name": "KCL FB ChatBot",
  "version": "1.0",
  "description": "FB ChatBot on chrome extension by HarryKwan",
  "manifest_version": 2,
  "permissions": [
    "activeTab","storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://m.facebook.com/*"],
      "js": ["content.js"]
    }
  ], "background": {
    "scripts": ["background.js"],
    "persistent": false
  },  "browser_action": {
    "default_popup": "start.html"

  },
  
  "options_page": "options.html"
}
